{
  "name": "functions",
  "version": "0.5.0",
  "scripts": {
    "build": "tsc -p .",
    "start": "npm run build && firebase serve --only functions",
    "watch": "tsc --pretty -w -p .",
    "logs": "firebase functions:log",
    "lint": "eslint --ext js,ts,tsx src",
    "dist": "npm run lint && npm run build",
    "config:get": "firebase functions:config:get > .runtimeconfig.json"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "etag": "^1.8.1",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.3.0",
    "got": "^11.8.0",
    "highlight.js": "^11.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.1",
    "rehype-highlight": "^5.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "server-timing": "^3.3.1",
    "tsimportlib": "^0.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/ejs": "^3.0.5",
    "@types/etag": "^1.8.0",
    "@types/express": "^4.11.1",
    "firebase-functions-test": "^0.3.2"
  },
  "private": true
}
